{
  "name": "extension-manager",
  "version": "1.0.0",
  "description": "A Chrome extension to manage other extensions",
  "type": "module",
  "scripts": {
    "dev": "NODE_ENV=development node --loader ts-node/esm node_modules/webpack/bin/webpack.js --config webpack.config.ts --mode=development --watch",
    "build": "NODE_ENV=production node --loader ts-node/esm node_modules/webpack/bin/webpack.js --config webpack.config.ts --mode=production",
    "start": "NODE_ENV=development node --loader ts-node/esm node_modules/webpack/bin/webpack.js --config webpack.config.ts --mode=development",
    "lint": "eslint .",
    "format-fix": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,scss}\"",
    "format-check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,css,scss}\"",
    "type-check": "tsc --noEmit",
    "clean-install-modules": "rm -rf node_modules && pnpm install"
  },
  "keywords": [
    "chrome-extension",
    "extension-manager",
    "typescript",
    "react"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@floating-ui/react": "^0.27.8",
    "@headlessui/react": "^2.2.3",
    "@heroicons/react": "^2.2.0",
    "@plasmohq/storage": "^1.3.0",
    "@types/react-window": "^1.8.8",
    "consola": "^3.4.2",
    "lucide-react": "^0.511.0",
    "react": "^18.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.5.2",
    "react-window": "^1.8.11",
    "tslog": "^4.9.3",
    "uuid": "^11.1.0",
    "zustand": "^5.0.4"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/chrome": "^0.0.260",
    "@types/copy-webpack-plugin": "^10.1.3",
    "@types/node": "^20.17.47",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@types/uuid": "^10.0.0",
    "@types/webpack": "^5.28.5",
    "@types/webpack-env": "^1.18.8",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "autoprefixer": "^10.4.21",
    "copy-webpack-plugin": "^13.0.0",
    "css-loader": "^7.1.2",
    "eslint": "^9.27.0",
    "postcss": "^8.5.3",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.5.3",
    "style-loader": "^4.0.0",
    "tailwindcss": "3.4.1",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "webpack": "^5.99.8",
    "webpack-cli": "^6.0.1"
  },
  "packageManager": "pnpm@10.11.0+sha512.6540583f41cc5f628eb3d9773ecee802f4f9ef9923cc45b69890fb47991d4b092964694ec3a4f738a420c918a333062c8b925d312f42e4f0c263eb603551f977"
}
