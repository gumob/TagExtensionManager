{
  "name": "extension-manager",
  "version": "1.0.0",
  "description": "A Chrome extension to manage other extensions",
  "type": "module",
  "main": "src/index.ts",
  "scripts": {
    "dev": "NODE_ENV=development NODE_OPTIONS=\"--loader ts-node/esm\" webpack --config webpack.config.ts --mode development --watch",
    "build": "NODE_ENV=production NODE_OPTIONS=\"--loader ts-node/esm\" webpack --config webpack.config.ts --mode production",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "chrome-extension",
    "extension-manager",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/chrome": "^0.0.323",
    "@types/node": "^20.17.47",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.2",
    "eslint": "^9.27.0",
    "prettier": "^3.5.3",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "webpack": "^5.99.8",
    "webpack-cli": "^5.1.4"
  },
  "packageManager": "pnpm@10.11.0+sha512.6540583f41cc5f628eb3d9773ecee802f4f9ef9923cc45b69890fb47991d4b092964694ec3a4f738a420c918a333062c8b925d312f42e4f0c263eb603551f977",
  "dependencies": {
    "@types/uuid": "^10.0.0",
    "uuid": "^11.1.0"
  }
}
