{
  "name": "extension-manager",
  "version": "1.0.0",
  "description": "A Chrome extension to manage other extensions",
  "type": "module",
  "main": "src/index.ts",
  "scripts": {
    "dev": "NODE_ENV=development NODE_OPTIONS=\"--loader ts-node/esm\" webpack --config webpack.config.ts --mode development --watch",
    "build": "NODE_ENV=production NODE_OPTIONS=\"--loader ts-node/esm\" webpack --config webpack.config.ts --mode production",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "chrome-extension",
    "extension-manager",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/chrome": "^0.0.260",
    "@types/node": "^20.17.31",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^6.10.0",
    "eslint": "^8.56.0",
    "prettier": "^3.5.3",
    "style-loader": "^3.3.4",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.0",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  },
  "packageManager": "pnpm@9.15.4+sha512.b2dc20e2fc72b3e18848459b37359a32064663e5627a51e4c74b2c29dd8e8e0491483c3abb40789cfd578bf362fb6ba8261b05f0387d76792ed6e23ea3b1b6a0",
  "dependencies": {
    "@types/uuid": "^10.0.0",
    "uuid": "^11.1.0"
  }
}
