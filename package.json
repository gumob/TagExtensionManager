{
  "name": "extension-manager",
  "version": "0.0.1",
  "description": "A Chrome extension to manage other extensions",
  "type": "module",
  "scripts": {
    "dev": "NODE_ENV=development node --loader ts-node/esm node_modules/webpack/bin/webpack.js --config webpack.config.ts --mode=development --watch",
    "build": "NODE_ENV=production node --loader ts-node/esm node_modules/webpack/bin/webpack.js --config webpack.config.ts --mode=production",
    "start": "NODE_ENV=development node --loader ts-node/esm node_modules/webpack/bin/webpack.js --config webpack.config.ts --mode=development",
    "prettier-check": "prettier --config prettier.config.js --check \"src/**/*.{js,jsx,ts,tsx,css,scss,json}\"",
    "prettier-fix": "prettier --config prettier.config.js --write \"src/**/*.{js,jsx,ts,tsx,css,scss,json}\"",
    "eslint-check": "eslint \"src/**/*.{ts,tsx,js,jsx}\"",
    "eslint-fix": "eslint --fix \"src/**/*.{ts,tsx,js,jsx}\"",
    "type-check": "tsc --noEmit",
    "clean-install-modules": "rm -rf node_modules && pnpm install"
  },
  "keywords": [
    "chrome-extension",
    "extension-manager",
    "typescript",
    "react"
  ],
  "author": "Kojiro Futamura",
  "license": "MIT",
  "dependencies": {
    "@floating-ui/react": "^0.27",
    "@headlessui/react": "^2.2",
    "@heroicons/react": "^2.2",
    "@plasmohq/storage": "^1.3",
    "@types/react-window": "^1.8",
    "consola": "^3.4",
    "lucide-react": "^0.511",
    "react": "^18.2",
    "react-dnd": "^16.0",
    "react-dnd-html5-backend": "^16.0",
    "react-dom": "^18.2",
    "react-hot-toast": "^2.5",
    "react-window": "^1.8",
    "tslog": "^4.9",
    "uuid": "^11.1",
    "zustand": "^5.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.29",
    "@trivago/prettier-plugin-sort-imports": "^5.2",
    "@types/chrome": "^0.0",
    "@types/html-webpack-plugin": "^3.2",
    "@types/node": "^20.17",
    "@types/react": "^18.3",
    "@types/react-dom": "^18.3",
    "@types/uuid": "^10.0",
    "@typescript-eslint/eslint-plugin": "^8.34",
    "@typescript-eslint/parser": "^8.34",
    "autoprefixer": "^10.4",
    "chrome-webstore-upload-cli": "^3.3.2",
    "copy-webpack-plugin": "^13.0",
    "css-loader": "^7.1",
    "eslint": "^9.29",
    "eslint-config-prettier": "^10.1",
    "eslint-plugin-import": "^2.32",
    "globals": "^16.2",
    "html-webpack-plugin": "^5.6",
    "mini-css-extract-plugin": "^2.9",
    "postcss": "^8.5",
    "postcss-loader": "^8.1",
    "prettier": "^3.5",
    "style-loader": "^4.0",
    "tailwindcss": "3.4.1",
    "ts-loader": "^9.5",
    "ts-node": "^10.9",
    "typescript": "^5.8",
    "webpack": "^5.99",
    "webpack-cli": "^6.0"
  },
  "packageManager": "pnpm@10.12.1+sha512.f0dda8580f0ee9481c5c79a1d927b9164f2c478e90992ad268bbb2465a736984391d6333d2c327913578b2804af33474ca554ba29c04a8b13060a717675ae3ac"
}
